{
  "name": "defender-os",
  "version": "0.0.1",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "Matthew Cobb",
  "homepage": "https://www.electronjs.org",
  "scripts": {
    "copy": "bash ./scripts/copy.sh",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "build:pi": "electron-vite build && electron-builder --linux AppImage --arm64 --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/rubik": "^5.0.18",
    "@fontsource/rubik-mono-one": "^5.0.18",
    "@mui/icons-material": "^5.15.5",
    "@mui/material": "^5.15.5",
    "@tomickigrzegorz/react-circular-progress-bar": "^1.1.2",
    "axios": "^1.6.5",
    "buffer": "^6.0.3",
    "electron-is-dev": "^3.0.1",
    "electron-typescript-ipc": "^3.0.0",
    "events": "^3.3.0",
    "jmuxer": "^2.0.5",
    "node-carplay": "github:rhysmorgan134/node-CarPlay",
    "pcm-player": "^0.0.17",
    "pcm-ringbuf-player": "github:rhysmorgan134/pcm-ringbuf-player#main",
    "react-gauge-component": "^1.1.30",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.21.3",
    "sass": "^1.70.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "stream-browserify": "^3.0.0",
    "usb": "^2.11.0",
    "vite-plugin-esmodule": "^1.5.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/events": "^3.0.3",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
